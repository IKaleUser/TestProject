import grt
#import mforms
CREATE SCHEMA `newdb` ;
CREATE TABLE `newdb`.`file_info` (
  `file_id` INT NOT NULL AUTO_INCREMENT,
  `file_name` VARCHAR(45) NOT NULL,
  `file_date` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`file_id`));